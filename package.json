{
	"name": "tamagui-storybook-expo",
	"version": "1.0.0",
	"private": true,
	"main": "index.js",
	"scripts": {
		"android": "expo start --android",
		"biome:check": "bun x @biomejs/biome check ./packages ./apps",
		"biome:fix": "bun x @biomejs/biome check --apply-unsafe ./packages ./apps",
		"biome:format": "bun x @biomejs/biome format --write ./components",
		"biome:lint": "bun x @biomejs/biome lint ./packages ./apps",
		"clean": "git clean -xdf node_modules && rm -f bun.lockb",
		"deps:update": "bunx npm-check-updates -ui",
		"expo:doctor": "bunx expo-doctor@latest",
		"expo:fix": "bunx expo install --fix",
		"gen": "bun generate component",
		"generate": "bun turbo gen",
		"postinstall": "bun run bun biome:format",
		"ios": "expo start --ios",
		"sb:build": "storybook build",
		"sb:generate": "sb-rn-get-stories",
		"start": "expo start",
		"up:storybook": "bunx npm-check-updates -ui -f '/storybook/'",
		"up:tamagui": "bunx npm-check-updates -ui -f '/tamagui/'",
		"web": "cross-env TAMAGUI_TARGET=web storybook dev -p 6006"
	},
	"dependencies": {
		"@babel/core": "^7.23.9",
		"@biomejs/biome": "^1.5.3",
		"@expo/metro-config": "^0.17.4",
		"@expo/metro-runtime": "^3.1.3",
		"@expo/vector-icons": "^14.0.0",
		"@react-native-async-storage/async-storage": "^1.21.0",
		"@react-native-community/datetimepicker": "7.6.1",
		"@react-native-community/slider": "4.4.2",
		"@react-navigation/native": "^6.1.9",
		"@storybook/addon-actions": "^7.6.17",
		"@storybook/addon-controls": "^7.6.17",
		"@storybook/addon-essentials": "^7.6.17",
		"@storybook/addon-links": "^7.6.17",
		"@storybook/addon-ondevice-actions": "^7.6.15",
		"@storybook/addon-ondevice-backgrounds": "^7.6.15",
		"@storybook/addon-ondevice-controls": "^7.6.15",
		"@storybook/addon-ondevice-notes": "^7.6.15",
		"@storybook/addon-react-native-web": "^0.0.23",
		"@storybook/react": "^7.6.17",
		"@storybook/react-native": "^7.6.15",
		"@storybook/react-webpack5": "^7.6.17",
		"@tamagui/animations-react-native": "^1.90.9",
		"@tamagui/babel-plugin": "^1.90.9",
		"@tamagui/build": "^1.90.9",
		"@tamagui/config": "^1.90.9",
		"@tamagui/font-inter": "^1.90.9",
		"@tamagui/react-native-media-driver": "^1.90.9",
		"@tamagui/shorthands": "^1.90.9",
		"@tamagui/theme-base": "^1.90.9",
		"@tamagui/themes": "^1.90.9",
		"@turbo/gen": "^1.12.3",
		"@types/react": "~18.2.45",
		"babel-loader": "^9.1.3",
		"babel-plugin-react-docgen-typescript": "^1.5.1",
		"cross-env": "^7.0.3",
		"expo": "~50.0.8",
		"expo-constants": "^15.4.5",
		"expo-font": "~11.10.3",
		"metro-minify-terser": "^0.80.5",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-native": "0.73.4",
		"react-native-safe-area-context": "4.8.2",
		"react-native-web": "^0.19.10",
		"storybook": "7.6.17",
		"tamagui": "^1.90.9",
		"tamagui-loader": "^1.90.9",
		"turbo": "^1.12.3",
		"typescript": "^5.3.3"
	}
}
